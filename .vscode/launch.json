{
	"version": "0.2.0",
	"configurations": [
	{
		"type": "chrome",
		"request": "launch",
		"name": "Start Webpack-Dev-Server",
		"url": "http://localhost:8087",
		"webRoot": "${workspaceFolder}/dist",
		"preLaunchTask": "npm: start",
		"runtimeArgs": [
			"--load-extension=${env:LOCALAPPDATA}\\Google\\Chrome\\User Data\\Default\\Extensions\\fmkadmapgofadopljbjfkapdkoienihi\\4.2.0_0"
		],
		"breakOnLoad": false,
		"breakOnLoadStrategy": "instrument",
		"sourceMaps": false,
		"smartStep": true,
		"showAsyncStacks": true,
		"sourceMapPathOverrides": {
			"webpack:///./*": "${webRoot}/../src/*",
			"webpack:///../*": "${webRoot}/*"
		}
	}]
}
